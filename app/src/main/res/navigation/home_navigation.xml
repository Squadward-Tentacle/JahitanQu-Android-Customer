<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/homeFragment">

    <action
        android:id="@+id/toHomeFragment"
        app:destination="@id/homeFragment"
        />

    <action
        android:id="@+id/toAccountFragment"
        app:destination="@id/accountFragment"
        />

    <action
        android:id="@+id/toMyOrderFragment"
        app:destination="@id/myOrderFragment"
        />

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.jahitanqu_customer.presentation.views.main.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.jahitanqu_customer.presentation.views.main.fragment.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/myOrderFragment"
        android:name="com.example.jahitanqu_customer.presentation.views.main.fragment.MyOrderFragment"
        android:label="fragment_my_order"
        tools:layout="@layout/fragment_my_order" />
</navigation>